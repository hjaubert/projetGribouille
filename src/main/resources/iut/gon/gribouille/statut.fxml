<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>



<GridPane
	fx:controller="iut.gon.gribouille.controleurs.ControleurStatut"
	prefHeight="26.0" prefWidth="830.0"
	xmlns:fx="http://javafx.com/fxml/1">
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
			prefWidth="100.0" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
			prefWidth="100.0" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
			prefWidth="100.0" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
			prefWidth="100.0" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
			prefWidth="100.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints minHeight="10.0" prefHeight="30.0"
			vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
		<Label fx:id="valeurOutil" GridPane.columnIndex="3" />
		<HBox prefHeight="100.0" prefWidth="200.0"
			GridPane.columnIndex="2">
			<children>
				<Label text="Epaisseur : " HBox.hgrow="SOMETIMES">
					<HBox.margin>
						<Insets top="5.0" />
					</HBox.margin>
				</Label>
				<Label fx:id="valeurEpaisseur">
					<HBox.margin>
						<Insets top="5.0" />
					</HBox.margin>
				</Label>
			</children>
		</HBox>
		<HBox prefHeight="100.0" prefWidth="200.0"
			GridPane.columnIndex="1">
			<children>
				<Label text="Y : ">
					<HBox.margin>
						<Insets top="5.0" />
					</HBox.margin>
				</Label>
				<Label fx:id="valeurY">
					<HBox.margin>
						<Insets top="5.0" />
					</HBox.margin>
				</Label>
			</children>
		</HBox>
		<HBox prefHeight="100.0" prefWidth="200.0">
			<children>
				<Label text="X : ">
					<HBox.margin>
						<Insets top="5.0" />
					</HBox.margin>
				</Label>
				<Label fx:id="valeurX" text=": ">
					<HBox.margin>
						<Insets top="5.0" />
					</HBox.margin>
				</Label>
			</children>
		</HBox>
		<Label fx:id="valeurCouleur" alignment="CENTER" contentDisplay="CENTER" text="Couleur"
			GridPane.columnIndex="4" />
	</children>
</GridPane>

